{
    "name": "era5_test",
    "phase": "train",
    "gpu_ids": [0],
    "path": {
        "log": "logs",
        "tb_logger": "tb_logger",
        "results": "results",
        "checkpoint": "checkpoint",
        "resume_state":null
    },
    "data": {
        "batch_size": 240,
        "num_workers": 6,
        "use_shuffle": true,
	"height": 240
    },
    "model": {
        "finetune_norm": false,
        "unet": {
            "in_channel": 4,  // data, land, mask, noisy
            "out_channel": 1, // data
            "inner_channel": 64, // channel
            "norm_groups": 32,  // normgroup
            "channel_multiplier": [1,1,2, 4], //
            "attn_res": [32,16],  // 
            "res_blocks": 1,
            "dropout": 0.1,
            "init_method": "kaiming"
        },
        "beta_schedule": {
            "train": {
                "schedule": "cosine",
                "n_timestep": 1000,
                "linear_start": 1e-6,
		        "linear_end": 1e-3
            },
            "val": {
                "schedule": "cosine",
                "n_timestep": 1000,
                "linear_start": 1e-6,
                "linear_end": 1e-3
            },
            "test": {
                "schedule": "cosine",
                "n_timestep": 1000,
                "linear_start": 1e-6,
                "linear_end": 1e-3
            }
        },
        "diffusion": {
            "conditional": true,
            "loss": "l2"
        }
    },
    "training": {
        "epoch_n_iter": 10000,
        "val_freq": 1000,
        "save_checkpoint_freq": 1000,
        "print_freq": 250,
        "n_val_vis": 1,
        "val_vis_freq": 100,
        "sample_size":30,
        "optimizer": {
            "type": "adamw",  // Possible types are ['adam', 'adamw']
            "amsgrad": false,
            "lr": 1e-4
        }
    }
}

